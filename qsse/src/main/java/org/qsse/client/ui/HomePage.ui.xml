<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="style.css" />
	<g:HTMLPanel styleName="{style.wrapper}">
		<div class="display-inline {style.container}">
		
			<div class="display-inline {style.header}">
				<g:SimplePanel ui:field="headerSimplePanel"></g:SimplePanel>
			</div>
			<div class="display-inline {style.content}">
				<div class="display-inline {style.contentWrapper}">			
					<g:HorizontalPanel ui:field="mainContent" width="100%">
					
					</g:HorizontalPanel>	
				</div>
			</div>
				
			<div class="display-inline {style.footer}">
				<g:SimplePanel ui:field="footerSimplePanel"></g:SimplePanel>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 