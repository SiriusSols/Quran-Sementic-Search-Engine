<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="style.css" />
	<g:HTMLPanel styleName="{style.headerWrapper}">
		<div class="display-inline {style.logo}"></div>
		<div class="display-inline {style.menubar}">
			<g:HorizontalPanel ui:field="menuBar" >
				<g:Hyperlink targetHistoryToken='home' ui:field="home">Home</g:Hyperlink>
				<g:Hyperlink targetHistoryToken='readquran' ui:field="readQuran">Read Quran</g:Hyperlink>
				<g:Hyperlink targetHistoryToken='searchQuran' ui:field="searchQuran">Search Quran</g:Hyperlink>
				<g:Hyperlink targetHistoryToken='articles' ui:field="articlesList">Articles</g:Hyperlink>
				<g:Hyperlink targetHistoryToken='quersionslist' ui:field="questionsList">Questions</g:Hyperlink>
				<g:Hyperlink targetHistoryToken='eventsList' ui:field="eventsList">Events</g:Hyperlink>
				<g:Hyperlink targetHistoryToken='userList' ui:field="usersList">Users</g:Hyperlink>
				<g:Hyperlink targetHistoryToken='login' ui:field="login">Login</g:Hyperlink>
				<g:Hyperlink targetHistoryToken='register' ui:field="register">Register</g:Hyperlink>
				<g:Hyperlink targetHistoryToken="contactus" ui:field="contactus">Contact Us</g:Hyperlink>
				<g:Hyperlink targetHistoryToken='logout' ui:field="logout">Logout</g:Hyperlink>
			</g:HorizontalPanel>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 